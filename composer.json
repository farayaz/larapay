{
  "name": "farayaz/larapay",
  "description": "iranian online payment gateways",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Farayaz\\Larapay\\": "src/"
    }
  },
  "homepage": "https://github.com/farayaz/larapay",
  "authors": [
    {
      "name": "Mehrdad Motaghifar",
      "email": "mehrdad.motaghifar@gmail.com",
      "role": "Developer"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Farayaz\\Larapay\\LarapayServiceProvider"
      ],
      "aliases": {
        "Larapay": "Farayaz\\Larapay\\LarapayFacade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "illuminate/container": "^10.0 || ^11.0 || ^12.0",
    "illuminate/contracts": "^10.0 || ^11.0 || ^12.0",
    "illuminate/support": "^10.0 || ^11.0 || ^12.0",
    "morilog/jalali": "3.*",
    "illuminate/http": "^10.0 || ^11.0 || ^12.0"
  },
  "require-dev": {
    "laravel/pint": "^1.0"
  },
  "scripts": {
    "format": "vendor/bin/pint"
  },
  "config": {
    "platform": {
      "php": "8.1"
    }
  }
}
